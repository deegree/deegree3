<?xml version="1.0" encoding="UTF-8"?>
<wms:deegreeWMS xmlns:wms="http://www.deegree.org/services/wms" xmlns:ds="http://www.deegree.org/datasource" xmlns:cm="http://www.deegree.org/commons" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dgws="http://www.deegree.org/webservices" configVersion="0.5.0" xsi:schemaLocation="http://www.deegree.org/services/wms ../../../schema/wms/0.5.0/wms_configuration.xsd">

  <wms:ServiceConfiguration>

    <wms:UnrequestableLayer>
      <wms:Title>CITE WMS root layer</wms:Title>
        <wms:CRS>EPSG:4326 EPSG:26912 EPSG:32230 CRS:84</wms:CRS>

      <wms:RequestableLayer>
        <wms:Name>raster</wms:Name>
        <wms:Title>raster</wms:Title>
        <ds:MultiResolutionDataSource crs="EPSG:26912">
          <ds:Resolution res="1.0">
            <ds:RasterDataSource>
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/1.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
          <ds:Resolution res="2.0">
            <ds:RasterDataSource>
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/2.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
          <ds:Resolution res="4.0">
            <ds:RasterDataSource>
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/4.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
          <ds:Resolution res="8.0">
            <ds:RasterDataSource>
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/8.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
          <ds:Resolution res="16.0">
            <ds:RasterDataSource>
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/16.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
          <ds:Resolution res="32.0">
            <ds:RasterDataSource>
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/32.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
          <ds:Resolution res="64.0">
            <ds:RasterDataSource crs="EPSG:26912">
              <ds:RasterDirectory fileType="jpg" filePattern=".jpg" recursive="true">
                ../../data/utah/raster/dop/64.0
              </ds:RasterDirectory>
            </ds:RasterDataSource>
          </ds:Resolution>
        </ds:MultiResolutionDataSource>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>interpolate</wms:Name>
        <wms:Title>interpolate</wms:Title>
        <ds:RasterDataSource crs="EPSG:26912">
          <ds:RasterFile fileType="tif">../../data/utah/raster/dem/dem.tif</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/interpolate.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>categorize</wms:Name>
        <wms:Title>categorize</wms:Title>
        <ds:RasterDataSource crs="EPSG:26912">
          <ds:RasterFile fileType="tif">../../data/utah/raster/dem/dem.tif</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/categorize.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>shaded</wms:Name>
        <wms:Title>shaded</wms:Title>
        <ds:RasterDataSource crs="EPSG:26912">
          <ds:RasterFile fileType="tif">../../data/utah/raster/dem/dem.tif</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/shaded.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>Shuffled Channels</wms:Name>
        <wms:Title>channel_shifts</wms:Title>
        <ds:RasterDataSource crs="EPSG:4326">
          <ds:RasterFile fileType="jpeg">../../data/RussianDoll.jpg</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/channel_shifts.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>Gamma Increase: 2.5</wms:Name>
        <wms:Title>gamma:2.5</wms:Title>
        <ds:RasterDataSource crs="EPSG:4326">
          <ds:RasterFile fileType="jpeg">../../data/RussianDoll.jpg</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/gamma25.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>Histogram Contrast Normalization</wms:Name>
        <wms:Title>histogram</wms:Title>
        <ds:RasterDataSource crs="EPSG:4326">
          <ds:RasterFile fileType="jpeg">../../data/RussianDoll.jpg</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/histogram.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>Regular Contrast Normalization</wms:Name>
        <wms:Title>normalize</wms:Title>
        <ds:RasterDataSource crs="EPSG:4326">
          <ds:RasterFile fileType="jpeg">../../data/RussianDoll.jpg</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/normalize.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

      <wms:RequestableLayer>
        <wms:Name>Shifted Channels + Histogram</wms:Name>
        <wms:Title>shift_histogram</wms:Title>
        <ds:RasterDataSource crs="EPSG:4326">
          <ds:RasterFile fileType="jpeg">../../data/RussianDoll.jpg</ds:RasterFile>
        </ds:RasterDataSource>
        <wms:DirectStyle>
          <wms:File>styles/shifts+histogram.sld</wms:File>
        </wms:DirectStyle>
      </wms:RequestableLayer>

    </wms:UnrequestableLayer>

  </wms:ServiceConfiguration>

</wms:deegreeWMS>
