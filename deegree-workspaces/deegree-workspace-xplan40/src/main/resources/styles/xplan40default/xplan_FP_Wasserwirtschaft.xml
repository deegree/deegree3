<?xml version="1.0" encoding="UTF-8"?>
<FeatureTypeStyle xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc" xmlns:sed="http://www.deegree.org/se"
    xmlns:deegreeogc="http://www.deegree.org/ogc"   xmlns:xplan="http://www.xplanung.de/xplangml/4/0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd http://www.deegree.org/se http://schemas.deegree.org/se/1.1.0/Symbolizer-deegree.xsd">
    <FeatureTypeName>xplan:FP_Wasserwirtschaft</FeatureTypeName>
     <!-- Fl채che -->
    <Rule>
        <Name>bp_</Name>
        <Description>
            <Title>FP: Wasserwirtschaftsfl채che</Title>
        </Description>
        <ogc:Filter>
          <ogc:PropertyIsEqualTo>
            <ogc:Function name="IsSurface">
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </ogc:Function>
            <ogc:Literal>true</ogc:Literal>
          </ogc:PropertyIsEqualTo>
        </ogc:Filter>
        <PolygonSymbolizer>
            <Geometry>
                <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <Stroke>
                <SvgParameter name="stroke">#B1B1B1</SvgParameter>
                <SvgParameter name="stroke-opacity">1.0</SvgParameter>
                <SvgParameter name="stroke-width">2</SvgParameter>
                <SvgParameter name="stroke-linecap">butt</SvgParameter>
                <SvgParameter name="stroke-linejoin">round</SvgParameter>
            </Stroke>
        </PolygonSymbolizer>
    </Rule>
    <!-- Linien -->
    <Rule>
        <Name>bp_</Name>
        <Description>
            <Title>FP: Wasserwirtschaftsfl채che</Title>
        </Description>
        <ogc:Filter>
          <ogc:PropertyIsEqualTo>
            <ogc:Function name="IsCurve">
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </ogc:Function>
            <ogc:Literal>true</ogc:Literal>
          </ogc:PropertyIsEqualTo>
        </ogc:Filter>
        <LineSymbolizer>
            <Geometry>
                <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <Stroke>
                <SvgParameter name="stroke">#B1B1B1</SvgParameter>
                <SvgParameter name="stroke-opacity">1.0</SvgParameter>
                <SvgParameter name="stroke-width">2</SvgParameter>
                <SvgParameter name="stroke-linecap">butt</SvgParameter>
                <SvgParameter name="stroke-linejoin">round</SvgParameter>
            </Stroke>
        </LineSymbolizer>
    </Rule>
    <!-- Punkt -->
    <Rule>
        <Name>bp_</Name>
        <Description>
            <Title>FP: Wasserwirtschaftsfl채che</Title>
        </Description>
        <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:Function name="IsPoint">
                <ogc:PropertyName>xplan:position</ogc:PropertyName>
              </ogc:Function>
              <ogc:Literal>true</ogc:Literal>
            </ogc:PropertyIsEqualTo>
        </ogc:Filter>
        <PointSymbolizer>
            <Geometry>
                <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <Graphic>
                <Mark>
                    <WellKnownName>circle</WellKnownName>
                    <Fill>
                        <SvgParameter name="fill">#B1B1B1</SvgParameter>
                        <SvgParameter name="fill-opacity">1.0</SvgParameter>
                    </Fill>
                    <Stroke>
                        <SvgParameter name="stroke">#B1B1B1</SvgParameter>
                        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
                        <SvgParameter name="stroke-width">2</SvgParameter>
                    </Stroke>
                </Mark>
                <Size>10</Size>
            </Graphic>
        </PointSymbolizer>
    </Rule>
</FeatureTypeStyle>