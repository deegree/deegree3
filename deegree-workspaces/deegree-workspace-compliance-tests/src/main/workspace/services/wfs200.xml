<deegreeWFS xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  configVersion="3.4.0"
  xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/services/wfs/3.4.0/wfs_configuration.xsd">
  <SupportedVersions>
    <Version>2.0.0</Version>
  </SupportedVersions>
  <FeatureStoreId>autos</FeatureStoreId>
  <FeatureStoreId>roadsegments</FeatureStoreId>
  <FeatureStoreId>streams</FeatureStoreId>
  <EnableTransactions>true</EnableTransactions>
  <QueryCRS>urn:ogc:def:crs:EPSG::4326</QueryCRS>
  <GMLFormat gmlVersion="GML_32">
    <MimeType>application/gml+xml; version=3.2</MimeType>
    <MimeType>application/xml; subtype="gml/3.2.1"</MimeType>
    <GetFeatureResponse>
      <DisableStreaming>true</DisableStreaming>
    </GetFeatureResponse>
  </GMLFormat>
</deegreeWFS>