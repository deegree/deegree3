<deegreeWFS xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" configVersion="3.4.0" xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/services/wfs/3.4.0/wfs_configuration.xsd">
  <SupportedVersions>
    <Version>1.1.0</Version>
  </SupportedVersions>
  <FeatureStoreId>cite110</FeatureStoreId>
  <EnableTransactions>true</EnableTransactions>
  <DisableResponseBuffering>false</DisableResponseBuffering>
  <QueryCRS>urn:ogc:def:crs:EPSG:6.11.2:4326</QueryCRS>
  <QueryCheckAreaOfUse>true</QueryCheckAreaOfUse>
  <GMLFormat gmlVersion="GML_31">
    <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
    <GetFeatureResponse>
      <DisableStreaming>true</DisableStreaming>
    </GetFeatureResponse>
  </GMLFormat>
</deegreeWFS>