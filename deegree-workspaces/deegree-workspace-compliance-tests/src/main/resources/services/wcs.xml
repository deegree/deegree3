<?xml version="1.0" encoding="UTF-8"?>
<deegreeWCS configVersion="3.4.0" xmlns="http://www.deegree.org/services/wcs" xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:md="http://www.deegree.org/webservices"
  xsi:schemaLocation="http://www.deegree.org/services/wcs http://schemas.deegree.org/services/wcs/3.4.0/wcs_configuration.xsd">
  <ServiceConfiguration>
    <SupportOptions>
      <OutputFormat>tiff</OutputFormat>
      <OutputFormat>jpeg</OutputFormat>
      <OutputFormat>bmp</OutputFormat>
      <Interpolation>Nearest-Neighbor</Interpolation>
      <Interpolation>Bilinear</Interpolation>
      <SupportedCRS>EPSG:4326</SupportedCRS>
    </SupportOptions>
    <Coverage>
      <Name>saltlakecity</Name>
      <Label>deegree demo dataset</Label>
      <CoverageStoreId>utah_dop</CoverageStoreId>
      <RangeSet>
        <Name>utah_ranges</Name>
        <AxisDescription name="bands">
          <AxisValue>
            <SingleValue>red</SingleValue>
            <SingleValue>green</SingleValue>
            <SingleValue>blue</SingleValue>
          </AxisValue>
        </AxisDescription>
      </RangeSet>
      <SupportOptions extend="true">
        <SupportedCRS>EPSG:26912</SupportedCRS>
      </SupportOptions>
    </Coverage>
    <Coverage>
      <Name>cite</Name>
      <Label>cite test data taken from wms 1.3.0</Label>
      <CoverageStoreId>terrain</CoverageStoreId>
      <SupportOptions extend="true">
        <SupportedCRS>EPSG:4314</SupportedCRS>
      </SupportOptions>
    </Coverage>
  </ServiceConfiguration>
  <PublishedInformation>
    <SupportedVersions>
      <Version>1.0.0</Version>
    </SupportedVersions>
    <AllowedOperations>
      <GetCapabilities />
      <DescribeCoverage />
      <GetCoverage />
    </AllowedOperations>
  </PublishedInformation>
</deegreeWCS>