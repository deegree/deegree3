<SQLFeatureStore xmlns="http://www.deegree.org/datasource/feature/sql" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" configVersion="3.4.0" xsi:schemaLocation="http://www.deegree.org/datasource/feature/sql http://schemas.deegree.org/datasource/feature/sql/3.4.0/sql.xsd">
  
  <!-- [1] Identifier of JDBC connection -->
  <JDBCConnId>osmdemo</JDBCConnId>
  
  <!-- [0...n] Definitions of feature types (derived from tables) -->
  
  <!-- Config option 1: Minimal configuration (just table) -->
  <!-- Implicit namespace (xmlns:app="http://www.deegree.org/app"), automatic property mapping (all table columns) -->
  <FeatureTypeMapping name="admin" table="osm_admin"/>
  <FeatureTypeMapping name="aeroways" table="osm_aeroways"/>
  <FeatureTypeMapping name="amenities" table="osm_amenities"/>
  <FeatureTypeMapping name="buildings" table="osm_buildings"/>
  <FeatureTypeMapping name="landusages" table="osm_landusages"/>
  <!--<FeatureTypeMapping table="osm_landusages_gen0"/>
  <FeatureTypeMapping table="osm_landusages_gen1"/>-->
  <!--    <FeatureTypeMapping name="mainroads" table="osm_mainroads"/>-->
  <!--<FeatureTypeMapping table="osm_mainroads_gen0"/>
  <FeatureTypeMapping table="osm_mainroads_gen1"/>-->
  <!--    <FeatureTypeMapping name="minorroads" table="osm_minorroads"/>
  <FeatureTypeMapping name="motorways" table="osm_motorways"/>-->
  <!--<FeatureTypeMapping table="osm_motorways_gen0"/>
  <FeatureTypeMapping table="osm_motorways_gen1"/>-->
  <FeatureTypeMapping name="places" table="osm_places"/>
  <FeatureTypeMapping name="railways" table="osm_railways"/>
  <!--<FeatureTypeMapping table="osm_railways_gen0"/>
  <FeatureTypeMapping table="osm_railways_gen1"/>-->
  <FeatureTypeMapping name="roads" table="osm_roads">
    <FIDMapping>
      <Column name="id"/>
    </FIDMapping>
  </FeatureTypeMapping>
  <FeatureTypeMapping name="roads_gen0" table="osm_roads_gen0">
    <FIDMapping>
      <Column name="id"/>
    </FIDMapping>
  </FeatureTypeMapping>
  <FeatureTypeMapping name="roads_gen1" table="osm_roads_gen1">
    <FIDMapping>
      <Column name="id"/>
    </FIDMapping>
  </FeatureTypeMapping>
  <FeatureTypeMapping name="transport_areas" table="osm_transport_areas"/>
  <FeatureTypeMapping name="transport_points" table="osm_transport_points"/>
  <FeatureTypeMapping name="waterareas" table="osm_waterareas"/>
  <!--<FeatureTypeMapping table="osm_waterareas_gen0"/>
  <FeatureTypeMapping table="osm_waterareas_gen1"/>-->
  <FeatureTypeMapping name="waterways" table="osm_waterways"/>
  
</SQLFeatureStore>