<?xml version="1.0" encoding="UTF-8"?>
<FeatureTypeStyle xmlns="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc" xmlns:sed="http://www.deegree.org/se"
  xmlns:deegreeogc="http://www.deegree.org/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd http://www.deegree.org/se http://schemas.deegree.org/se/1.1.0/Symbolizer-deegree.xsd">
  <Name>DEM</Name>
  <Rule>
    <Name>DEM 30m Raster</Name>
    <Description>
      <Title>DEM 30m Elevation Data</Title>
    </Description>
    <MinScaleDenominator>50000</MinScaleDenominator>
    <MaxScaleDenominator>300000000</MaxScaleDenominator>
    <RasterSymbolizer>
      <Opacity>0.5</Opacity>
      <ColorMap>
        <Interpolate fallbackValue="#dddddd" method="color">
          <LookupValue>Rasterdata</LookupValue>
          <InterpolationPoint>
            <Data>-32767</Data>
            <Value>#000000FF</Value>
          </InterpolationPoint>
          <InterpolationPoint>
            <Data>0</Data>
            <Value>#000000FF</Value>
          </InterpolationPoint>
          <InterpolationPoint>
            <Data>1</Data>
            <Value>#00FF00</Value>
          </InterpolationPoint>
          <InterpolationPoint>
            <Data>1000</Data>
            <Value>#FFFF00</Value>
          </InterpolationPoint>
          <InterpolationPoint>
            <Data>2000</Data>
            <Value>#630002</Value>
          </InterpolationPoint>
          <InterpolationPoint>
            <Data>3000</Data>
            <Value>#A16F70</Value>
          </InterpolationPoint>
          <InterpolationPoint>
            <Data>60000</Data>
            <Value>#FFFFFF</Value>
          </InterpolationPoint>
        </Interpolate>
      </ColorMap>
    </RasterSymbolizer>
  </Rule>
  <Rule>
    <Name>DEM 30m Raster</Name>
    <Description>
      <Title>DEM 30m Elevation Data</Title>
    </Description>
    <MinScaleDenominator>50000</MinScaleDenominator>
    <MaxScaleDenominator>300000000</MaxScaleDenominator>
    <RasterSymbolizer>
      <Opacity>0.5</Opacity>
      <ShadedRelief>
        <BrightnessOnly>false</BrightnessOnly>
        <ReliefFactor>55</ReliefFactor>
      </ShadedRelief>
    </RasterSymbolizer>
  </Rule>
