<?xml version="1.0" encoding="UTF-8"?>
<ProcessDefinition configVersion="3.4.0" processVersion="1.0.0" storeSupported="true" statusSupported="false"
  xmlns="http://www.deegree.org/processes/java" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/processes/java http://schemas.deegree.org/processes/java/3.4.0/java.xsd">
  <Identifier>Union</Identifier>
  <JavaClass>org.deegree.wps.jts.UnionProcesslet</JavaClass>
  <Title>Calculates the union of two GML Geometries.</Title>
  <Abstract>The union of two Geometries A and B is the set of all points which lie in A or B.</Abstract>
  <InputParameters>
    <ComplexInput>
      <Identifier>GMLInput1</Identifier>
      <Title>GMLInput1</Title>
      <DefaultFormat mimeType="text/xml" encoding="UTF-8" schema="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd" />
    </ComplexInput>
    <ComplexInput>
      <Identifier>GMLInput2</Identifier>
      <Title>GMLInput2</Title>
      <DefaultFormat mimeType="text/xml" encoding="UTF-8" schema="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd" />
    </ComplexInput>
  </InputParameters>
  <OutputParameters>
    <ComplexOutput>
      <Identifier>Union</Identifier>
      <Title>Union</Title>
      <DefaultFormat mimeType="text/xml" encoding="UTF-8" schema="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd" />
    </ComplexOutput>
  </OutputParameters>
</ProcessDefinition>