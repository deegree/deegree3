<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>deegree-workspace-inspire</artifactId>
  <packaging>deegree-workspace</packaging>
  <name>deegree-workspace-inspire</name>
  <description>Example configuration for setting up INSPIRE View and Download Services using deegree WFS and WMS</description>

  <properties>
    <deegree.module.status>check</deegree.module.status>
  </properties>

  <parent>
    <groupId>org.deegree</groupId>
    <artifactId>deegree-workspaces</artifactId>
    <version>3.2-pre9-SNAPSHOT</version>
  </parent>

  <repositories>
    <repository>
      <id>deegree-repo</id>
      <url>http://artefacts.deegree.org/repo</url>
      <releases>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>
  </repositories>

  <dependencies>
    <dependency>
      <groupId>org.deegree</groupId>
      <artifactId>deegree-workspace-base</artifactId>
      <version>${project.version}</version>
      <type>deegree-workspace</type>
    </dependency>
  </dependencies>

</project>

