  <simpleType name="IsolationValueType">
    <annotation>
      <documentation>-- Definition --&#13;
A set of codes to represent the degree of isolation present in the aggregation unit in relation to the natural range of the species.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="populationIsolated">
        <annotation>
          <documentation>-- Definition --&#13;
The population is isolated or almost isolated.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="populationNotIsolatedButMarginal">
        <annotation>
          <documentation>-- Definition --&#13;
The population is not isolated, but on the margins of the area of distribution.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="populationNotIsolatedAndInExtendedRange">
        <annotation>
          <documentation>-- Definition --&#13;
The population is not isolated and is within an extended distribution range.</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="SpeciesGroupValueType">
    <annotation>
      <documentation>-- Definition --&#13;
A code indicating the broad group within which the species falls.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="birds">
        <annotation>
          <documentation>-- Definition --&#13;
The species is a type of bird.</documentation>
