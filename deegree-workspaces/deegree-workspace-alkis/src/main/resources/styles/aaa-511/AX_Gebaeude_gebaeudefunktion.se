				<ExternalGraphic>
					<OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink"
						xlink:type="simple" xlink:href="../../symbols/Kreditinstitut.png" />
					<Format>png</Format>
				</ExternalGraphic>
				<Size>5</Size>
			</Graphic>
		</PointSymbolizer>
