<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.45.1326" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">[E] 'Gewässerachse' repräsentiert eine Wasserfläche, die Bestandteil des topologischen Gewässernetzes ist. 

</description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_Gewaesserachse</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1327">
      <description xmlns="http://www.opengis.net/gml/3.2">'Breite des Gewässers' ist die Breite von 'Gewässerachse' in [m] gemäß Klassenangabe. 
Die Breite von Gewässern kann sich kontinuierlich ändern. Als 'Breite des Gewässers' ist eine durchschnittliche Breite für einen längeren Streckenabschnitt anzugeben. Als längerer Streckenabschnitt gilt eine Strecke von &gt;= 500 m.	
</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:breiteDesGewaessers</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">breiteDesGewaessers</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>Integer</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1328">
      <description xmlns="http://www.opengis.net/gml/3.2">'Hydrologisches Merkmal' gibt die Wasserverhältnisse von 'Gewässerachse' an.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:hydrologischesMerkmal</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">hydrologischesMerkmal</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1328_S.164.1805.45.1341">
          <description xmlns="http://www.opengis.net/gml/3.2">'Nicht ständig Wasser führend' heißt, dass ein Gewässer nicht ganzjährig Wasser führt.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:hydrologischesMerkmal:2000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Nicht ständig Wasser führend</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1328_S.164.1805.45.1342">
          <description xmlns="http://www.opengis.net/gml/3.2">'Trocken, versiegt' heißt, dass ein Gewässerbett ganzjährig kein Wasser führt.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:hydrologischesMerkmal:3000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Trocken, versiegt</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_HydrologischesMerkmal_Gewaesserachse</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1329">
      <description xmlns="http://www.opengis.net/gml/3.2">'Funktion' ist die Art von 'Gewässerachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:funktion</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">funktion</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1329_S.164.1805.45.1335">
          <description xmlns="http://www.opengis.net/gml/3.2">'Kanal' ist ein für die Schifffahrt angelegter künstlicher Wasserlauf.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:funktion:8300</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Kanal</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Funktion_Gewaesserachse</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1330">
      <description xmlns="http://www.opengis.net/gml/3.2">'Zustand' beschreibt die Betriebsbereitschaft von 'Gewässerachse' mit FKT 8300.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:zustand</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">zustand</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1330_S.164.1805.45.1324">
          <description xmlns="http://www.opengis.net/gml/3.2">'Außer Betrieb, stillgelegt, verlassen' bedeutet, dass sich der Kanal nicht mehr in regelmäßiger, der Bestimmung entsprechenden Nutzung befindet.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:zustand:2100</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Außer Betrieb, stillgelegt, verlassen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1330_S.164.1805.45.1325">
          <description xmlns="http://www.opengis.net/gml/3.2">'Im Bau' bedeutet, dass der Kanal noch nicht fertiggestellt ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:zustand:4000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Im Bau</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Zustand_Kanal</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1331">
      <description xmlns="http://www.opengis.net/gml/3.2">'Fließrichtung' ist die Richtung in der das Wasser fließt. Die Richtung der Geometrie der Gewässerachse und die der Fließrichtung ist dabei identisch (true).</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserachse:fliessrichtung</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">fliessrichtung</name>
      <cardinality>1</cardinality>
      <valueTypeName>Boolean</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>featureType</classification>
  <supertypeRef xlink:href="urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:TA_CurveComponent"/>
</TypeDefinition>
