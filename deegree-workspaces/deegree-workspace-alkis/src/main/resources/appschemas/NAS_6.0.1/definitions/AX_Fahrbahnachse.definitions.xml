<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.45.1909" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">[E] 'Fahrbahnachse' beschreibt die Geometrie und Eigenschaften einer Fahrbahn bei mehrbahnigen Straßen. Zur Fahrbahn gehören auch Stand- und Kriechspuren. Die begeh- und befahrbare Fläche einer Fußgängerzone ist als Fahrbahn anzusehen.

</description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_Fahrbahnachse</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1910">
      <description xmlns="http://www.opengis.net/gml/3.2">'Besondere Fahrstreifen' beschreibt die begleitenden Fuß- und Radwege von 'Fahrbahnachse', sofern sie nicht mehr als 3 m vom Fahrbahnrand entfernt verlaufen.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:besondereFahrstreifen</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">besondereFahrstreifen</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1910_S.164.1805.45.1906">
          <description xmlns="http://www.opengis.net/gml/3.2">'Mit Radweg' bedeutet, dass parallel zur &lt;Objektart&gt; ein Radweg verläuft, der aber nicht als eigenständiges Objekt erfasst wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:besondereFahrstreifen:1000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Mit Radweg</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1910_S.164.1805.45.1907">
          <description xmlns="http://www.opengis.net/gml/3.2">'Mit Fußweg' bedeutet, dass parallel zur &lt;Objektart&gt; ein Fußweg verläuft, der aber nicht als eigenständiges Objekt erfasst wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:besondereFahrstreifen:2000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Mit Fußweg</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1910_S.164.1805.45.1908">
          <description xmlns="http://www.opengis.net/gml/3.2">'Mit Rad- und Fußweg' bedeutet, dass parallel zur &lt;Objektart&gt; ein Rad- und Fußweg verläuft, der aber nicht als eigenständiges Objekt erfasst wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:besondereFahrstreifen:3000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Mit Rad- und Fußweg</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_BesondereFahrstreifen</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1911">
      <description xmlns="http://www.opengis.net/gml/3.2">'Breite der Fahrbahn' ist die Breite der befestigten Fläche in [m] auf 0,5 m gerundet. Bei Fußgängerzonen wird als 'Breite der Fahrbahn' die Breite der begehbaren Fläche angegeben.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:breiteDerFahrbahn</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">breiteDerFahrbahn</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>Length</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1912">
      <description xmlns="http://www.opengis.net/gml/3.2">'Funktion' beschreibt die verkehrliche Nutzung von 'Fahrbahnachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:funktion</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">funktion</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1912_S.164.1805.45.1917">
          <description xmlns="http://www.opengis.net/gml/3.2">'Fußgängerzone' ist ein dem Fußgängerverkehr vorbehaltener Bereich, in dem ausnahmsweise öffentlicher Personenverkehr, Lieferverkehr oder Fahrradverkehr zulässig sein kann. </description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:funktion:1808</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Fußgängerzone</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Funktion_Fahrbahnachse</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1913">
      <description xmlns="http://www.opengis.net/gml/3.2">'Anzahl der Fahrstreifen' ist die tatsächliche Anzahl der Fahrstreifen von 'Fahrbahnachse'. Standstreifen sind keine Fahrstreifen.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:anzahlDerFahrstreifen</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">anzahlDerFahrstreifen</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>Integer</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1914">
      <description xmlns="http://www.opengis.net/gml/3.2">'Zustand' beschreibt die Betriebsbereitschaft von 'Fahrbahnachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:zustand</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">zustand</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1914_S.164.1805.45.1852">
          <description xmlns="http://www.opengis.net/gml/3.2">'Außer Betrieb, stillgelegt, verlassen' bedeutet, dass sich &lt;Objektart&gt; nicht mehr in regelmäßiger, der Bestimmung entsprechenden Nutzung befindet.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:zustand:2100</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Außer Betrieb, stillgelegt, verlassen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1914_S.164.1805.45.1853">
          <description xmlns="http://www.opengis.net/gml/3.2">'Im Bau' bedeutet, dass &lt;Objektart&gt; noch nicht fertiggestellt ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:zustand:4000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Im Bau</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Zustand</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1915">
      <description xmlns="http://www.opengis.net/gml/3.2">'Oberflächenmaterial' ist die Fahrbahnbefestigung von 'Fahrbahnachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:oberflaechenmaterial</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">oberflaechenmaterial</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1915_S.164.1805.45.1919">
          <description xmlns="http://www.opengis.net/gml/3.2">'Beton' bedeutet, dass die Oberfläche von &lt;Objektart&gt; aus Beton besteht.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:oberflaechenmaterial:1220</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Beton</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1915_S.164.1805.45.1920">
          <description xmlns="http://www.opengis.net/gml/3.2">'Bitumen, Asphalt' bedeutet, dass die Oberfläche von &lt;Objektart&gt; aus Bitumen bzw. Asphalt besteht.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:oberflaechenmaterial:1230</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Bitumen, Asphalt</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1915_S.164.1805.45.1921">
          <description xmlns="http://www.opengis.net/gml/3.2">'Pflaster' bedeutet, dass die Oberfläche von &lt;Objektart&gt; gepflastert ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:oberflaechenmaterial:1240</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Pflaster</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1915_S.164.1805.45.1922">
          <description xmlns="http://www.opengis.net/gml/3.2">'Gestein, zerkleinert' bedeutet, dass die Oberfläche von &lt;Objektart&gt; aus Schotter, Splitt, Sand oder aus einem Gemisch dieser Materialen besteht.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Fahrbahnachse:oberflaechenmaterial:1250</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Gestein, zerkleinert</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Oberflaechenmaterial_Strasse</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>featureType</classification>
  <supertypeRef xlink:href="urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:TA_CurveComponent"/>
</TypeDefinition>
