<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.46.3543" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">"AX_K_GRUPPE" enthält die reservierten Fachkennzeichen und gruppiert sie nach Nummerierungsbezirken bzw. Gebietskennungen.
</description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_K_GRUPPE</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_K_GRUPPE</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3544">
      <description xmlns="http://www.opengis.net/gml/3.2">"Nummerierungsbezirk" enthält den in Abhängigkeit vom Inhalt des Attributes "Art der Fachkennzeichen" der Ausgabeobjektart "Liste der reservierten Fachkennzeichen" festgelegten Nummerierungsbezirk bzw Gebietskennung als Teil der Fachkennzeichen. 

Folgende Nummerierungsbezirke können vorkommen:
- Kilometerquadrat bei Punktkennungen,
- Gemarkung-Flur bei Flurstückskennzeichen,
- Gemarkung bei Fortführungsnummern,
- Gemarkung bei Abmarkungsprotokollnummern.
</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_K_GRUPPE:nummerierungsbezirk</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">nummerierungsbezirk</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3545">
      <description xmlns="http://www.opengis.net/gml/3.2">"Reserviert" enthält die Reservierten Ordnungsnummern mit zusätzlichen Angaben.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_K_GRUPPE:reserviert</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">reserviert</name>
      <cardinality>1..*</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_K_RESERVIERT"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>dataType</classification>
</TypeDefinition>
