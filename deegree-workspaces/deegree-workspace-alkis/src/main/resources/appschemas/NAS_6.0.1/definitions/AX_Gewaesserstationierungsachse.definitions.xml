<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.46.307" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">'Gewässerstationierungsachse' ist eine von einer Wasserfachstelle festgelegte Linie in Gewässern.</description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_Gewaesserstationierungsachse</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.314">
      <description xmlns="http://www.opengis.net/gml/3.2">Angaben zur Herkunft der Informationen (Erhebungsstelle, Art der Datenerhebung).</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:qualitaetsangaben</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">qualitaetsangaben</name>
      <cardinality>0..1</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_DQMitDatenerhebung"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.308">
      <description xmlns="http://www.opengis.net/gml/3.2">'Art der Gewässerachse' beschreibt die Festlegung von 'Gewässerstationierungsachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:artDerGewaesserachse</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">artDerGewaesserachse</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.308_S.164.1805.46.321">
          <description xmlns="http://www.opengis.net/gml/3.2">'Gewässerachse der WSV' ist eine Gewässerachse, deren Geometrie unverändert aus den Unterlagen der Wasser- und Schifffahrtsverwaltung übernommen wurde.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:artDerGewaesserachse:1000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Gewässerachse der WSV</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.308_S.164.1805.46.322">
          <description xmlns="http://www.opengis.net/gml/3.2">'Genäherte Mittellinie in Gewässern' ist eine Gewässerachse, die den Spezifikationen der Richtlinie der 'Gebiets- und Gewässerverschlüsselung' der Länderarbeitsgemeinschaft Wasser (LAWA) entspricht.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:artDerGewaesserachse:2000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Genäherte Mittellinie in Gewässern</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.308_S.164.1805.46.323">
          <description xmlns="http://www.opengis.net/gml/3.2">'Fiktive Verbindung in Fließgewässern' ist eine Gewässerachse, die ein einmündendes Gewässer mit der Gewässerachse des aufnehmenden Fließgewässers verbindet.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:artDerGewaesserachse:3001</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Fiktive Verbindung in Fließgewässern</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.308_S.164.1805.46.324">
          <description xmlns="http://www.opengis.net/gml/3.2">'Fiktive Verbindung in Seen und Teichen' ist eine hydrologisch sinnvolle Verbindungslinie in stehenden Gewässern, die für den Aufbau eines geschlossenen topologischen Gewässernetzes benötigt wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:artDerGewaesserachse:3002</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Fiktive Verbindung in Seen und Teichen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_ArtDerGewaesserachse</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.309">
      <description xmlns="http://www.opengis.net/gml/3.2">'Name' ist der Eigenname von 'Gewässerstationierungsachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:name</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">name</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.310">
      <description xmlns="http://www.opengis.net/gml/3.2">'Gewässerkennzahl' ist die von der Fachverwaltung vergebene Verschlüsselung von 'Gewässerstationierungsachse'.
Die Gewässerkennzahl beschreibt in der 
  1.- 19. Stelle die Gewässerkennzahl laut LAWA und in der 
  20.- 29. Stelle die Gebiets- oder Abschnittskennung zur Gewässerkennzahl.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:gewaesserkennzahl</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">gewaesserkennzahl</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.311">
      <description xmlns="http://www.opengis.net/gml/3.2">'Zweitname' ist der touristische bzw. volkstümliche  Name von 'Gewässerstationierungsachse'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:zweitname</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">zweitname</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.312">
      <description xmlns="http://www.opengis.net/gml/3.2">'Identnummer' ist die von der Wasser- und Schifffahrtsverwaltung vergebene Verschlüsselung des Gewässers.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:identnummer</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">identnummer</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.313">
      <description xmlns="http://www.opengis.net/gml/3.2">'Fließrichtung' ist die Richtung in der das Wasser fließt. Die Richtung der Geometrie der Gewässerstationierungsachse und die der Fließrichtung ist dabei indentisch (true).</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:fliessrichtung</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">fliessrichtung</name>
      <cardinality>1</cardinality>
      <valueTypeName>Boolean</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="G.206">
      <description xmlns="http://www.opengis.net/gml/3.2">
      </description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Gewaesserstationierungsachse:inversZu_gehoertZuBauwerk</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">inversZu_gehoertZuBauwerk</name>
      <cardinality>0..*</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_SonstigesBauwerkOderSonstigeEinrichtung"/>
      <type>associationRole</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>featureType</classification>
  <supertypeRef xlink:href="urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:TA_CurveComponent"/>
</TypeDefinition>
