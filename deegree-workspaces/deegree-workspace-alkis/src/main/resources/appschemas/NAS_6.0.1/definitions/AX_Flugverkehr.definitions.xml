<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.45.1799" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">[E] 'Flugverkehr' umfasst die baulich geprägte Fläche und die mit ihr in Zusammenhang stehende Freifläche, die ausschließlich oder vorwiegend dem Flugverkehr dient.</description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_Flugverkehr</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1800">
      <description xmlns="http://www.opengis.net/gml/3.2">'Funktion' ist die zum Zeitpunkt der Erhebung vorherrschende Nutzung (Dominanzprinzip).</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:funktion</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">funktion</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1800_S.164.1805.45.1807">
          <description xmlns="http://www.opengis.net/gml/3.2">'Gebäude- und Freifläche zu Verkehrsanlagen, Luftfahrt' ist eine besondere Flugverkehrsfläche.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:funktion:5501</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Gebäude- und Freifläche zu Verkehrsanlagen, Luftfahrt</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Funktion_Flugverkehr</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801">
      <description xmlns="http://www.opengis.net/gml/3.2">'Art' ist Einstufung der Flugverkehrsfläche durch das Luftfahrtbundesamt.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">art</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1939">
          <description xmlns="http://www.opengis.net/gml/3.2">'Flughafen' ist eine Anlage mit Gebäuden, Bauwerken, Start- und Landebahnen sowie sonstigen  flugtechnischen Einrichtungen zur Abwicklung des Flugverkehrs.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5510</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Flughafen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1940">
          <description xmlns="http://www.opengis.net/gml/3.2">'Internationaler Flughafen' ist ein Flughafen, der in der Luftfahrtkarte 1 : 500000 (ICAO) als solcher ausgewiesen ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5511</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Internationaler Flughafen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1941">
          <description xmlns="http://www.opengis.net/gml/3.2">'Regionalflughafen' ist ein Flughafen der gemäß Raumordnungsgesetz als Regionalflughafen eingestuft ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5512</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Regionalflughafen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1942">
          <description xmlns="http://www.opengis.net/gml/3.2">'Verkehrslandeplatz' ist ein Flugplatz, der in der Luftfahrtkarte 1:500000 (ICAO) als solcher ausgewiesen ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5520</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Verkehrslandeplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1943">
          <description xmlns="http://www.opengis.net/gml/3.2">'Hubschrauberflugplatz' ist ein Flugplatz, der in der Luftfahrtkarte 1:500000 (ICAO) als solcher ausgewiesen ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5530</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Hubschrauberflugplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1944">
          <description xmlns="http://www.opengis.net/gml/3.2">'Landeplatz, Sonderlandeplatz' ist eine Fläche, die in der Luftfahrtkarte 1:500000 (ICAO) als Landeplatz, Sonderlandeplatz ausgewiesen ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5540</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Landeplatz, Sonderlandeplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1801_S.164.1805.45.1945">
          <description xmlns="http://www.opengis.net/gml/3.2">'Segelfluggelände' ist eine Fläche, die in der Luftfahrtkarte 1:500000 (ICAO) als Segelfluggelände ausgewiesen ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:art:5550</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Segelfluggelände</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Art_Flugverkehr</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1802">
      <description xmlns="http://www.opengis.net/gml/3.2">'Name' ist der Eigenname von 'Flugverkehr'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:name</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">name</name>
      <cardinality>0..1</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:unionType:GeoInfoDok::adv:6.0.1:AX_Lagebezeichnung"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1803">
      <description xmlns="http://www.opengis.net/gml/3.2">'Bezeichnung' ist die von einer Fachstelle vergebene Kennziffer von 'Flugverkehr'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:bezeichnung</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">bezeichnung</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1804">
      <description xmlns="http://www.opengis.net/gml/3.2">'Nutzung' gibt den Nutzerkreis von 'Flugverkehr' an.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:nutzung</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">nutzung</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1804_S.164.1805.45.1809">
          <description xmlns="http://www.opengis.net/gml/3.2">'Zivil' bedeutet, dass 'Flugverkehr' privaten, öffentlichen oder religiösen Zwecken dient und nicht militärisch genutzt wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:nutzung:1000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Zivil</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1804_S.164.1805.45.1810">
          <description xmlns="http://www.opengis.net/gml/3.2">'Militärisch' bedeutet, dass 'Flugverkehr' nur von Streitkräften genutzt wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:nutzung:2000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Militärisch</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1804_S.164.1805.45.1811">
          <description xmlns="http://www.opengis.net/gml/3.2">'Teils zivil, teils militärisch' bedeutet dass ''Flugverkehr' sowohl zivil als auch militärisch genutzt wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:nutzung:3000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Teils zivil, teils militärisch</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Nutzung_Flugverkehr</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1805">
      <description xmlns="http://www.opengis.net/gml/3.2">'Zustand' beschreibt die Betriebsbereitschaft von 'Flugverkehr'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:zustand</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">zustand</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1805_S.164.1805.45.1813">
          <description xmlns="http://www.opengis.net/gml/3.2">'Außer Betrieb, stillgelegt; verlassen' bedeutet, dass sich 'Flugverkehr' nicht mehr in regelmäßiger, der Bestimmung entsprechenden Nutzung befindet.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:zustand:2100</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Außer Betrieb, stillgelegt, verlassen</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1805_S.164.1805.45.1814">
          <description xmlns="http://www.opengis.net/gml/3.2">'Im Bau' bedeutet, dass 'Flugverkehr' noch nicht fertiggestellt ist.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Flugverkehr:zustand:4000</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Im Bau</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Zustand_Flugverkehr</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>featureType</classification>
  <supertypeRef xlink:href="urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_TatsaechlicheNutzung"/>
</TypeDefinition>
