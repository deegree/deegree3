<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.46.3613" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">
  </description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_Recht</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_Recht</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3614">
      <description xmlns="http://www.opengis.net/gml/3.2">'Buchung' ist die 'Buchungsstelle' und das 'Buchungsblatt', auf die das Flurstück verweist (Ordnungsmerkmal). 'Buchungsblatt' ist eine der Bestands-Objektarten 'Grundbuchblatt', 'Katasterblatt', 'Pseudoblatt', 'Erwerberblatt' oder 'Fiktives Blatt'. 

</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Recht:buchung</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">buchung</name>
      <cardinality>1</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_Buchung"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3615">
      <description xmlns="http://www.opengis.net/gml/3.2">'Buchungsart' bezeichnet die Art der Buchung (Entschlüsslte Werteart der Bestandsobjektart 'Buchungsstelle').

Bei der Blattart 'Katasterblatt' (Werteart 2000) wird zusätzlich der Hinweis 'Im Grundstück nicht gebucht' ausgegeben. </description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Recht:buchungsart</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">buchungsart</name>
      <cardinality>1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3616">
      <description xmlns="http://www.opengis.net/gml/3.2">'Anteil' enthält den unter einer Buchungsstelle auf einem Grundbuchblatt gebuchten Miteigentumsanteil. </description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Recht:anteil</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">anteil</name>
      <cardinality>0..1</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_Anteil"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3617">
      <description xmlns="http://www.opengis.net/gml/3.2">'Aufgeteilter Anteil' enthält den unter einer Buchungsstelle auf einem Fiktiven Blatt gebuchten Miteigentumsanteil. </description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Recht:aufgeteilterAnteil</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">aufgeteilterAnteil</name>
      <cardinality>0..1</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_Anteil"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.46.3618">
      <description xmlns="http://www.opengis.net/gml/3.2">'Rechtsinhaber' enthält die Angaben über den (die) Berechtigten eines grundstücksgleichen Rechtes.. </description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Recht:rechtsinhaber</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">rechtsinhaber</name>
      <cardinality>1..*</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:dataType:GeoInfoDok::adv:6.0.1:AX_K_PERS1"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>dataType</classification>
</TypeDefinition>
