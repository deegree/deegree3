      <valueTypeRef xlink:href="urn:x-shapechange:def:unionType:GeoInfoDok::adv:6.0.1:AA_Fachdatenobjekt"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>dataType</classification>
</TypeDefinition>
