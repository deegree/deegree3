      <valueTypeName>AX_Bauwerksfunktion_Transportanlage</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2079">
      <description xmlns="http://www.opengis.net/gml/3.2">'Produkt' gibt an, welches Produkt transportiert wird.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:produkt</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">produkt</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2079_S.164.1805.45.2087">
          <description xmlns="http://www.opengis.net/gml/3.2">'Erdöl' ist ein flüssiges und brennbares Kohlenwasserstoffgemisch, das gefördert oder transportiert wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:produkt:1110</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Erdöl</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2079_S.164.1805.45.2088">
          <description xmlns="http://www.opengis.net/gml/3.2">'Gas' ist eine gasförmige und brennbare Materie, die gefördert oder transportiert wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:produkt:1120</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Gas</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2079_S.164.1805.45.2089">
          <description xmlns="http://www.opengis.net/gml/3.2">'Wasser' ist die chemische Verbindung von Wasserstoff mit Sauerstoff, die gefördert oder transportiert wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:produkt:1130</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Wasser</name>
