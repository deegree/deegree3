        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2079_S.164.1805.45.2092">
          <description xmlns="http://www.opengis.net/gml/3.2">'Abwasser' ist ein verunreinigtes Wasser aus Haushaltungen, Gewerbe- und Industriebetrieben sowie Niederschlagswasser, das transportiert wird.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:produkt:1133</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Abwasser</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2079_S.164.1805.45.2093">
          <description xmlns="http://www.opengis.net/gml/3.2">
          </description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:produkt:1140</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Fernwärme</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Produkt_Transportanlage</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2080">
      <description xmlns="http://www.opengis.net/gml/3.2">'Lage zur Erdoberfläche' ist die Angabe der relativen Lage von 'Transportanlage' zur Erdoberfläche. </description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:lageZurErdoberflaeche</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">lageZurErdoberflaeche</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2080_S.164.1805.45.2095">
          <description xmlns="http://www.opengis.net/gml/3.2">'Unter der Erdoberfläche' bedeutet, dass sich die Transportanlage unter der Erdoberfläche befindet.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:lageZurErdoberflaeche:1200</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Unter der Erdoberfläche</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2080_S.164.1805.45.2096">
          <description xmlns="http://www.opengis.net/gml/3.2">'Aufgeständert' bedeutet, dass eine Transportanlage durch Tragwerke (Stützen, Pfeiler) über das Niveau der Erdoberfläche geführt wird, um z.B. Hindernisse zu überwinden. </description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:lageZurErdoberflaeche:1400</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Aufgeständert</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.2080_S.164.1805.45.2097">
          <description xmlns="http://www.opengis.net/gml/3.2">'Unter der Wasseroberfläche' bedeutet, dass sich eine Transportanlage unter der Wasseroberfläche befindet.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Transportanlage:lageZurErdoberflaeche:1700</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Unter der Wasseroberfläche</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_LageZurErdoberflaeche_Transportanlage</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
