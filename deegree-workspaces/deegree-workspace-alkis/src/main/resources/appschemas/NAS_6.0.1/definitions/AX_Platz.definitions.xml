<?xml-stylesheet type='text/xsl' href='./ShapeChangeDefinitionsType.xsl'?><TypeDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:def="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="S.164.1805.45.1760" xsi:schemaLocation="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5 ShapeChangeDefinitions.xsd">
  <description xmlns="http://www.opengis.net/gml/3.2">[E] 'Platz' ist eine Verkehrsfläche in Ortschaften oder eine ebene, befestigte oder unbefestigte Fläche, die bestimmten Zwecken dient (z. B. für Verkehr, Märkte, Festveranstaltungen).</description>
  <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_Platz</identifier>
  <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">AX_Platz</name>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761">
      <description xmlns="http://www.opengis.net/gml/3.2">'Funktion' ist die zum Zeitpunkt der Erhebung objektiv erkennbare oder feststellbare vorkommende Nutzung.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">funktion</name>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761_S.164.1805.45.1766">
          <description xmlns="http://www.opengis.net/gml/3.2">'Fußgängerzone' ist ein dem Fußgängerverkehr vorbehaltener Bereich, in dem ausnahmsweise öffentlicher Personenverkehr, Lieferverkehr oder Fahrradverkehr zulässig sein kann. </description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion:5130</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Fußgängerzone</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761_S.164.1805.45.1767">
          <description xmlns="http://www.opengis.net/gml/3.2">'Parkplatz' ist eine zum vorübergehenden Abstellen von Fahrzeugen bestimmte Fläche. </description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion:5310</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Parkplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761_S.164.1805.45.1768">
          <description xmlns="http://www.opengis.net/gml/3.2">'Rastplatz' ist eine Anlage zum Halten, Parken oder Rasten der Verkehrsteilnehmer mit unmittelbarem Anschluss zur Straße ohne Versorgungseinrichtung, ggf. mit Toiletten.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion:5320</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Rastplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761_S.164.1805.45.1769">
          <description xmlns="http://www.opengis.net/gml/3.2">'Raststätte' ist eine Anlage an Verkehrsstraßen mit Bauwerken und Einrichtungen zur Versorgung und 
Erholung von Reisenden.  
</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion:5330</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Raststätte</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761_S.164.1805.45.1770">
          <description xmlns="http://www.opengis.net/gml/3.2">
          </description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion:5340</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Marktplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
        <ListedValueDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1761_S.164.1805.45.1771">
          <description xmlns="http://www.opengis.net/gml/3.2">'Festplatz' ist eine Fläche, auf der zeitlich begrenzte Festveranstaltungen stattfinden.</description>
          <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:funktion:5350</identifier>
          <name xmlns="http://www.opengis.net/gml/3.2">Festplatz</name>
        </ListedValueDefinition>
      </dictionaryEntry>
      <cardinality>0..1</cardinality>
      <valueTypeName>AX_Funktion_Platz</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1762">
      <description xmlns="http://www.opengis.net/gml/3.2">'Name' ist der Eigenname von 'Platz'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:name</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">name</name>
      <cardinality>0..1</cardinality>
      <valueTypeRef xlink:href="urn:x-shapechange:def:unionType:GeoInfoDok::adv:6.0.1:AX_Lagebezeichnung"/>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1763">
      <description xmlns="http://www.opengis.net/gml/3.2">'Straßenschlüssel' ist das Gemeindekennzeichen ergänzt um eine Ordnungsnummer, die in der Regel von der Gemeinde vergeben wird.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:strassenschluessel</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">strassenschluessel</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <dictionaryEntry xmlns="http://www.opengis.net/gml/3.2">
    <PropertyDefinition xmlns="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5" gml:id="S.164.1805.45.1764">
      <description xmlns="http://www.opengis.net/gml/3.2">'Zweitname' ist der touristische oder volkstümliche Name von 'Platz'.</description>
      <identifier xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5">urn:x-shapechange:def:propertyType:GeoInfoDok::adv:6.0.1:AX_Platz:zweitname</identifier>
      <name xmlns="http://www.opengis.net/gml/3.2" codeSpace="http://www.interactive-instruments.de/ShapeChange/Definitions/0.5/name">zweitname</name>
      <cardinality>0..1</cardinality>
      <valueTypeName>CharacterString</valueTypeName>
      <type>attribute</type>
    </PropertyDefinition>
  </dictionaryEntry>
  <classification>featureType</classification>
  <supertypeRef xlink:href="urn:x-shapechange:def:featureType:GeoInfoDok::adv:6.0.1:AX_TatsaechlicheNutzung"/>
</TypeDefinition>
