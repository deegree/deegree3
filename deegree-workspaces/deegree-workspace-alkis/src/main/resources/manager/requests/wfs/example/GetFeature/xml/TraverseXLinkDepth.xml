<?xml version="1.0" encoding="UTF-8"?>
<wfs:GetFeature version="1.1.0" xmlns:adv="http://www.adv-online.de/namespaces/adv/gid/6.0" xmlns:wfs="http://www.opengis.net/wfs"
  xmlns:gml="http://www.opengis.net/gml" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/wfs
http://schemas.opengis.net/wfs/1.1.0/wfs.xsd" outputFormat="text/xml; subtype=gml/3.2.1"
  traverseXlinkDepth="*">
  <!--
     Selektiert das Flurstueck mit der Flurstuecksnummer 351/3 und gibt alle
     Unterobjekte im gleichen Dokument zurÃ¼ck (traverseXlinkDepth="*") 
   -->
  <wfs:Query typeName="adv:AX_Flurstueck">
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>adv:flurstuecksnummer/adv:AX_Flurstuecksnummer/adv:zaehler</ogc:PropertyName>
          <ogc:Literal>351</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>adv:flurstuecksnummer/adv:AX_Flurstuecksnummer/adv:nenner</ogc:PropertyName>
          <ogc:Literal>3</ogc:Literal>
        </ogc:PropertyIsEqualTo>
      </ogc:And>
    </ogc:Filter>
  </wfs:Query>
</wfs:GetFeature>
