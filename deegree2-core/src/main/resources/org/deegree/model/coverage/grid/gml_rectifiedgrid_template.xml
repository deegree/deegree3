<gml:RectifiedGridCoverage xmlns:gml="http://www.opengis.net/gml">
	<gml:rectifiedGridDomain>
		<gml:RectifiedGrid dimension="2">
			<gml:limits>
				<gml:GridEnvelope>
					<gml:low></gml:low>
					<gml:high></gml:high>
				</gml:GridEnvelope>
			</gml:limits>
			<gml:axisName>x</gml:axisName>
			<gml:axisName>y</gml:axisName>
			<gml:origin>
				<gml:Point>
					<gml:pos></gml:pos>
				</gml:Point>
			</gml:origin>
			<gml:offsetVector></gml:offsetVector>
			<gml:offsetVector></gml:offsetVector>
		</gml:RectifiedGrid>
	</gml:rectifiedGridDomain>
	<gml:rangeSet>
		<gml:File>
			<gml:rangeParameters>
				<gml:CompositeValue>
					<gml:valueComponents>
						<Elevation></Elevation>
					</gml:valueComponents>
				</gml:CompositeValue>
			</gml:rangeParameters>
			<gml:fileName></gml:fileName>
			<gml:fileStructure>Record Interleaved</gml:fileStructure>
		</gml:File>
	</gml:rangeSet>
</gml:RectifiedGridCoverage>
