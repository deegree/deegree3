<?xml version="1.0" encoding="UTF-8"?>
<wps:Execute xmlns:wps="http://www.opengis.net/wps/1.0.0"
  xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xlink="http://www.w3.org/1999/xlink" service="WPS" version="1.0.0"
  xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsExecute_request.xsd">
  <ows:Identifier>st_transform</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>LAYER</ows:Identifier>
      <wps:Data>
        <wps:ComplexData
          schema="http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd"
          mimeType="text/xml">
          <gml:LineString gml:id="Polygon1"
            xmlns="http://www.opengis.net/gml" srsName="EPSG:4326"
            xmlns:gml="http://www.opengis.net/gml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.opengis.net/gml http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd">
            <gml:posList>100 100 200 200</gml:posList>
          </gml:LineString>
        </wps:ComplexData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>DISTANCEX</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">300</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>DISTANCEY</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">300</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>ANGLE</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">180</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>SCALEX</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">1</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>SCALEY</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">1</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>ANCHORX</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">0</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>ANCHORY</ows:Identifier>
      <wps:Data>
        <wps:LiteralData dataType="double">0</wps:LiteralData>
      </wps:Data>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:ResponseDocument storeExecuteResponse="false"
      lineage="false" status="false">
      <wps:Output encoding="UTF-8"
        schema="http://schemas.opengis.net/gml/3.1.1/base/geometryComplexes.xsd"
        mimeType="text/xml">
        <ows:Identifier>RESULT</ows:Identifier>
      </wps:Output>
    </wps:ResponseDocument>
  </wps:ResponseForm>
</wps:Execute>
