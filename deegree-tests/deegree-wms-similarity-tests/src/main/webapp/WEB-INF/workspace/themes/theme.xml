    </Theme>
    <Theme>
      <Identifier>vegetationfiltersort</Identifier>
      <d:Title>vegetationfiltersort</d:Title>
      <Layer layerStore='vegetationfiltersort'>dominant</Layer>
    </Theme>
    <Theme>
      <Identifier>satellite_provo</Identifier>
      <d:Title>satellite_provo</d:Title>
      <Layer>satellite_provo</Layer>
    </Theme>
    <Theme>
      <Identifier>satellite_provo_scalelimit</Identifier>
      <d:Title>satellite_provo with scale limit</d:Title>
      <Layer>satellite_provo_scalelimit</Layer>
    </Theme>
