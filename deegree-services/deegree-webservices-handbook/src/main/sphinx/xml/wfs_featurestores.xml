<deegreeWFS configVersion="3.4.0"
  xmlns="http://www.deegree.org/services/wfs"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/services/wfs
  http://schemas.deegree.org/services/wfs/3.4.0/wfs_configuration.xsd">

  <!-- QueryCRS [1...n]: Announced CRS, first element is the default CRS -->
  <QueryCRS>urn:ogc:def:crs:EPSG::4258</QueryCRS>

  <!-- FeatureStoreId [0...n]: Ids of feature stores to use, omitted: all active feature stores-->
  <FeatureStoreId>countries</FeatureStoreId>
  <FeatureStoreId>cities</FeatureStoreId>
  <FeatureStoreId>rivers</FeatureStoreId>

</deegreeWFS>
