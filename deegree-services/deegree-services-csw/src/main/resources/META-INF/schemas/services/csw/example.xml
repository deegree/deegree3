<?xml version="1.0" encoding="UTF-8"?>
<deegreeCSW configVersion="3.4.0"
  xmlns="http://www.deegree.org/services/csw" xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/services/csw http://schemas.deegree.org/3.5/services/csw/csw_configuration.xsd">

  <!-- [0...1] Activated OGC protocol versions, default: latest implementation 
    of CSW spec - 2.0.2 -->
  <SupportedVersions>
    <Version>2.0.2</Version>
  </SupportedVersions>

  <!-- [0...1] Enable transactions (CSW operations), default: disabled -->
  <EnableTransactions>true</EnableTransactions>

  <!-- [0...1] Enable the INSPIRE extensions, default: disabled -->
  <EnableInspireExtensions />

</deegreeCSW>