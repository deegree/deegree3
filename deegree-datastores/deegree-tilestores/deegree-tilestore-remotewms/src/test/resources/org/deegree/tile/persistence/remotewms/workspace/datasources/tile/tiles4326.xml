<?xml version="1.0"?>
<RemoteWMSTileStore xmlns="http://www.deegree.org/datasource/tile/remotewms" xmlns:md="http://www.deegree.org/metadata/spatial"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/datasource/tile/remotewms http://schemas.deegree.org/datasource/tile/remotewms/3.4.0/remotewms.xsd"
  configVersion="3.4.0">

  <!-- [1]: Id of the remote OWS to connect to (must be a WMS instance) -->
  <RemoteWMSId>utah-wms</RemoteWMSId>

  <TileDataSet>
    <Identifier>tiles4326</Identifier>
    <TileMatrixSetId>inspirecrs84quad</TileMatrixSetId>
    <OutputFormat>image/png</OutputFormat>
    <RequestParams>
      <!-- [1] Comma-separated list of layer names -->
      <Layers>countyboundaries,countynames</Layers>
      <!-- [0..1] Comma-separated list of styles -->
      <Styles>default,default</Styles>
      <!-- [1] Image format -->
      <Format>image/png</Format>
      <CRS>EPSG:4326</CRS>
    </RequestParams>
  </TileDataSet>

</RemoteWMSTileStore>
