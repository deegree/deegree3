<?xml version="1.0"?>
<RemoteWMTSTileStore xmlns="http://www.deegree.org/datasource/tile/remotewmts" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/datasource/tile/remotewmts http://schemas.deegree.org/datasource/tile/remotewmts/3.4.0/remotewmts.xsd"
  configVersion="3.4.0">
  <!-- [1] Id of the remote WMTS to connect to -->
  <RemoteWMTSId>wmts</RemoteWMTSId>
 
  <TileDataSet>
    <!-- [0..1] Identifier of the local tile matrix set, defaults to the value of the TileMatrixSet request parameter below -->
    <TileMatrixSetId>googlecrs84quad</TileMatrixSetId>  
    <!-- [1] WMTS request parameters -->
    <RequestParams>
      <!-- [1] Layer name -->
      <Layer>medford:buildings</Layer>
      <!-- [1] Style identifier -->
      <Style>_null</Style>
      <!-- [1] Image format -->
      <Format>image/png</Format>
      <!-- [1] Identifier of the remote tile matrix set -->
      <TileMatrixSet>EPSG:900913</TileMatrixSet>
    </RequestParams>
  </TileDataSet>
</RemoteWMTSTileStore>
