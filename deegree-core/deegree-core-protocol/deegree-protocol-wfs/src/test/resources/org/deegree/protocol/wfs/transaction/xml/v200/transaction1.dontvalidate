    </fes:Filter>
  </wfs:Delete>
  <!-- Delete all instances of the feature type BuiltUpA_1M where: 1. the geometry is INSIDE a polygonal window 2. where 
    the POPULATION is between 100 and 2000 -->
  <wfs:Delete typeName="BuiltUpA_1M">
    <fes:Filter>
      <fes:And>
        <fes:Within>
          <fes:ValueReference>BuiltUpA_1M/bndry</fes:ValueReference>
          <gml:Polygon gml:id="WINDOW" srsName="urn:ogc:def:crs:EPSG::4326">
            <gml:exterior>
