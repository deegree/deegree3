									<xsd:element ref="mic:zeitreihe"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="nicht_wohnberechtigte_zuwanderer">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element ref="mic:zeitreihe"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="summe_zuwanderer_wohnungslose">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element ref="mic:zeitreihe"/>
